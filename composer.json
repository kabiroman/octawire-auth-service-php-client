{
    "name": "kabiroman/octawire-auth-service-php-client",
    "description": "PHP client for Octawire Auth Service using JATP (JSON Auth Transport Protocol) over TCP",
    "type": "library",
    "version": "0.9.1",
    "keywords": ["auth", "jwt", "tcp", "octawire"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-sockets": "*"
    },
    "conflict": {
        "grpc/grpc": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "predis/predis": "^2.0"
    },
    "suggest": {
        "predis/predis": "Required for Redis-based key caching"
    },
    "autoload": {
        "psr-4": {
            "Kabiroman\\Octawire\\AuthService\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kabiroman\\Octawire\\AuthService\\Client\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

